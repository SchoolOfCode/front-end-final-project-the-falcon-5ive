con5ives.herokuapp.com/users`, {\n      method: \"POST\",\n      body: JSON.stringify(newUser),\n      headers: { \"Content-Type\": \"application/json\" },\n    })\n      .then((res) => res.json())\n      .then((data) => console.log(\"this is the user data: \", data))\n      .catch((error) => console.log(\"user creation error error: \", error));\n\n    setComplete(true);\n  }\n\n  if (loading) {\n    return <Loading />;\n  }\n\n  // Renders journey signup form once user sign up is complete\n  if (complete) {\n    return <CreateJourney signup={signup} setSignup={setSignup} />;\n  }\n\n  return authUser ? (\n    <div>\n      <h1>Sign Up</h1>\n      <button onClick={logout}>Return to Home</button>\n      <div>\n        <form onSubmit={handleSubmit(createUser)}>\n          <span>\n            <img src={authUser?.photoURL} alt=\"user profile\" />\n          </span>\n          <span>\n            <p>Admin:</p>\n            <select name=\"admin\" ref={register}>\n              <option value=\"true\">Yes</option>\n              <option value=\"false\">No</option>\n            </select>\n          </span>\n          <span>\n            <p>Name:</p>\n            <input name=\"name\" ref={register} required />\n          </span>\n          <span>\n            <p>Surname:</p>\n            <input name=\"surname\" ref={register} required />\n          </span>\n          <span>\n            <p>Email:</p>\n            <input\n              name=\"email\"\n              ref={register}\n              required\n              defaultValue={authUser.email}\n            />\n          </span>\n          <span>\n            <p>Cohort:</p>\n            <select name=\"cohort\" ref={register}>\n              <option value=\"1\">1</option>\n              <option value=\"2\">2</option>\n              <option value=\"3\">3</option>\n              <option value=\"4\">4</option>\n            </select>\n          </span>\n          <span>\n            <p>Current Role:</p>\n            <input name=\"currentRole\" ref={register} required />\n          </span>\n          <span>\n            <p>Current Employer:</p>\n            <input name=\"currentEmployer\" ref={register} required />\n          </span>\n          <span>\n            <p>Skills:</p>\n            <input\n              name=\"skills\"\n              onChange={(e) => setSkillInput(e.target.value)}\n              value={skillInput}\n            />\n            <button onClick={(e) => addToSkills(e)}>Add Skill</button>\n            <ul>\n              {skills.map((item, index) => {\n                return (\n                  <div>\n                    <li key={`${item}${index}`}>{item}</li>\n                    <button\n                      onClick={(e) => {\n                        deleteSkill(index, e);\n                      }}\n                    >\n                      X\n                    </button>\n                  </div>\n                );\n              })}\n            </ul>\n          </span>\n          <span>\n            <p>10 Second Intro:</p>\n            <textarea name=\"introduction\" ref={register} required />\n          </span>\n          <span>\n            <p>Social Links:</p>\n            <label for=\"linkedin\">Linkedin: </label>\n            <input\n              name=\"linkedin\"\n              placeholder=\"https://example.com\"\n              pattern=\"https://.*\"\n              ref={register}\n            ></input>\n\n            <label for=\"Github\">Github: </label>\n            <input\n              name=\"github\"\n              placeholder=\"https://example.com\"\n              pattern=\"https://.*\"\n              ref={register}\n            ></input>\n\n            <label for=\"Twitter\">Twitter: </label>\n            <input\n              name=\"twitter\"\n              placeholder=\"https://example.com\"\n              pattern=\"https://.*\"\n              ref={register}\n            ></input>\n\n            <label for=\"Portfolio\">Portfolio: </label>\n 