        <Grid item xs={12}>\r\n                <InputLabel id=\"demo-simple-select-label\">Cohort</InputLabel>\r\n                <FormControl variant=\"outlined\" fullWidth>\r\n                  <Controller\r\n                    name=\"cohort\"\r\n                    as={\r\n                      <Select\r\n                        labelId=\"demo-simple-select-label\"\r\n                        id=\"demo-simple-select\"\r\n                        className={classes.formControl}\r\n                        name=\"cohort\"\r\n                        ref={register}\r\n                      >\r\n                        <MenuItem value=\"1\">1</MenuItem>\r\n                        <MenuItem value=\"2\">2</MenuItem>\r\n                        <MenuItem value=\"3\">3</MenuItem>\r\n                        <MenuItem value=\"4\">4</MenuItem>\r\n                        <MenuItem value=\"5\">5</MenuItem>\r\n                      </Select>\r\n                    }\r\n                    control={control}\r\n                    rules={{ required: \"Required\" }}\r\n                  />\r\n                </FormControl>\r\n              </Grid>\r\n\r\n              {/*----------Admin----------*/}\r\n              {/* <Grid item xs={12} sm={6}>\r\n                <InputLabel id=\"demo-simple-select-label\">Admin</InputLabel>\r\n                <FormControl variant=\"outlined\" fullWidth>\r\n                  <Controller\r\n                    name=\"admin\"\r\n                    as={\r\n                      <Select\r\n                        labelId=\"demo-simple-select-label\"\r\n                        id=\"demo-simple-select\"\r\n                        className={classes.formControl}\r\n                        name=\"admin\"\r\n                        ref={register}\r\n                      >\r\n                        <MenuItem value=\"yes\">Yes</MenuItem>\r\n                        <MenuItem value=\"no\">No</MenuItem>\r\n                      </Select>\r\n                    }\r\n                    control={control}\r\n                    rules={{ required: \"Required\" }}\r\n                  />\r\n                </FormControl>\r\n              </Grid> */}\r\n\r\n              {/*----------Current Role----------*/}\r\n              <Grid item xs={12} sm={6}>\r\n                <FormControl variant=\"outlined\" fullWidth>\r\n                  <Controller\r\n                    name=\"currentRole\"\r\n                    as={\r\n                      <TextField\r\n                        id=\"currentRole\"\r\n                        label=\"Current Role\"\r\n                        fullWidth\r\n                        required\r\n                      />\r\n                    }\r\n                    control={control}\r\n                    rules={{ required: \"Required\" }}\r\n                  />\r\n                </FormControl>\r\n              </Grid>\r\n\r\n              {/*----------Current Employer----------*/}\r\n              <Grid item xs={12} sm={6}>\r\n                <FormControl variant=\"outlined\" fullWidth>\r\n                  <Controller\r\n                    name=\"currentEmployer\"\r\n                    as={\r\n                      <TextField\r\n                        id=\"currentEmployer\"\r\n                        label=\"Current Employer\"\r\n                        fullWidth\r\n                        required\r\n                      />\r\n                    }\r\n                    control={control}\r\n                    rules={{ required: \"Required\" }}\r\n                  />\r\n                </FormControl>\r\n              </Grid>\r\n\r\n              {/*----------Skills Input----------*/}\r\n              <Grid item xs={12} sm={6}>\r\n                <TextField\r\n                  id=\"skills\"\r\n                  label=\"Skills\"\r\n                  fullWidth\r\n                  onChange={(e) => setSkillInput(e.target.value)}\r\n                />\r\n                <div className=\"addSkillButtonAligner\">\r\n                  <button onClick={(e) => addToSkills(e)} className=\"button\">\r\n                    Add Skill\r\n                  </button>\r\n                </div>\r\n              </Grid>\r\n\r\n              {/*----------Skills----------*/}\r\n              <Grid item xs={12} sm={6}>\r\n                <FormControl variant=\"outlined\" fullWidth>\r\n                  <Controller\r\n                    name=\"skills\"\r\n                    as={\r\n                      <ul>\r\n                        <div className=\"root\">\r\n                          {skills.map((item, index) => {\r\n                            return (\r\n                              <Tags\r\n                                key={`${item}${index}`}\r\n                                item={item}\r\n                                index={index}\r\n                                deleteSkill={deleteSkill}\r\n                              />\r\n                            );\r\n                          })}\r\n                        </div>\r\n                      </ul>\r\n                    }\r\n                    control={control}\r\n                    // rules={{ required: \"Required\" }}\r\n                  />\r\n                </FormControl>\r\n              </Grid>\r\n\r\n              {/*----------Intro----------*/}\r\n              <Grid item xs={12}>\r\n                <FormControl variant=\"outlined\" fullWidth>\r\n                  <Controller\r\n                    name=\"introduction\"\r\n                    as={\r\n                      <TextField\r\n                        id=\"introduction\"\r\n                        label=\"10 Second Intro...\"\r\n                        fullWidth\r\n                        multiline\r\n                        rows={3}\r\n                        required\r\n                      />\r\n                    }\r\n                    control={control}\r\n                    rules={{ required: \"Required\" }}\r\n                  />\r\n                </FormControl>\r\n              </Grid>\r\n\r\n              <Grid item xs={12} sm={6}>\r\n                <FormControl variant=\"outlined\" fullWidth>\r\n                  <Controller\r\n                    name=\"linkedin\"\r\n                    placeholder=\"https://example.com\"\r\n                    pattern=\"https://.*\"\r\n                    as={\r\n                      <TextField\r\n                        id=\"linkedin\"\r\n                        label=\"LinkedIn\"\r\n                        fullWidth\r\n                        required\r\n                      />\r\n                    }\r\n                    control={control}\r\n                    rules={{ required: \"Required\" }}\r\n                  />\r\n                </FormControl>\r\n              </Grid>\r\n\r\n              <Grid item xs={12} sm={6}>\r\n                <FormControl variant=\"outlined\" fullWidth>\r\n                  <Controller\r\n                    name=\"github\"\r\n                    placeholder=\"https://example.com\"\r\n                    pattern=\"https://.*\"\r\n                    as={<TextField id=\"github\" label=\"Github\" fullWidth />}\r\n                    control={control}\r\n                  />\r\n                </FormControl>\r\n              </Grid>\r\n\r\n              <Grid item xs={12} sm={6}>\r\n                <FormControl variant=\"outlined\" fullWidth>\r\n                  <Controller\r\n                    name=\"twitter\"\r\n                    placeholder=\"https://example.com\"\r\n                    pattern=\"https://.*\"\r\n                    as={<TextField id=\"twitter\" label=\"Twitter\" fullWidth />}\r\n                    control={control}\r\n                  />\r\n                </FormControl>\r\n              </Grid>\r\n\r\n              <Grid item xs={12} sm={6}>\r\n                <FormControl variant=\"outlined\" fullWidth>\r\n                  <Controller\r\n                    name=\"portfolio\"\r\n                    placeholder=\"https://example.com\"\r\n                    pattern=\"https://.*\"\r\n                    as={\r\n                      <TextField id=\"portfolio\" label=\"Portfolio\" fullWidth />\r\n                    }\r\n                    control={control}\r\n                  />\r\n                </FormControl>\r\n              </Grid>\r\n\r\n              <Grid item xs={12}>\r\n                <FormControl variant=\"outlined\" fullWidth>\r\n                  <Controller\r\n                    name=\"other\"\r\n                    placeholder=\"https://example.com\"\r\n                    pattern=\"https://.*\"\r\n                    as={<TextField id=\"other\" label=\"Other\" fullWidth />}\r\n                    control={control}\r\n                  />\r\n                </FormControl>\r\n              </Grid>\r\n              <Grid item xs={12}>\r\n                <FormControl variant=\"outlined\">\r\n                  <FormControlLabel\r\n                    control={\r\n                      <Checkbox\r\n                        checked={emailSub}\r\n                        onChange={() => setEmailSub(!emailSub)}\r\n                        inputProps={{ \"aria-label\": \"primary checkbox\" }}\r\n                      />\r\n                    }\r\n                    label={`Receive event email notifications so you don't miss anything SoC related! ${\r\n                      emailSub ? \"😄\" : \"😭 \"\r\n                    }`}\r\n                  />\r\n                  <p\r\n                    style={{\r\n                      margin: 0,\r\n                      padding: 0,\r\n                      fontSize: \"0.75rem\",\r\n                      color: \"rgba(0,0,0,0.5)\",\r\n                    }}\r\n                  >\r\n                    You'll only receive emails when an event is created, changed\r\n                    or cancelled, so that you can stay in touch with all of us\r\n                    here at the School of Code. We might also send a checkup\r\n                    email from time-to-time just to see how you're getting on.\r\n                    Don't worry you can change your preference later.\r\n                  </p>\r\n                </FormControl>\r\n              </Grid>\r\n            </Grid>\r\n          </React.Fragment>\r\n\r\n          <div className=\"signupSubmit\">\r\n            <div className=\"signupSubmitAligner\">\r\n              {/* Submit form button */}\r\n              <button\r\n                onClick={logout}\r\n                className=\"button-cancel buttonMarginRight\"\r\n              >\r\n                Return to Login\r\n              </button>\r\n              <input\r\n                type=\"submit\"\r\n                value=\"Next\"\r\n                className=\"button buttonMarginLeft\"\r\n              />\r\n            </div>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  ) : (\r\n    <Redirect to={\"/login\"}></Redirect>\r\n  );\r\n}\r\n","C:\\Users\\herzallr\\Desktop\\Me\\School of code\\Notes and code\\Week 13, 14, 15, 16 - project month\\code\\frontend - falcon5ive\\src\\Helpers\\createUserObj.js",[],"C:\\Users\\herzallr\\Desktop\\Me\\School of code\\Notes and code\\Week 13, 14, 15, 16 - project month\\code\\frontend - falcon5ive\\src\\Pages\\SignInUser\\background.js",[],"C:\\Users\\herzallr\\Desktop\\Me\\School of code\\Notes and code\\Week 13, 14, 15, 16 - project month\\code\\frontend - falcon5ive\\src\\Components\\CohortContainer\\cohortContainer.js",[],"C:\\Users\\herzallr\\Desktop\\Me\\School of code\\Notes and code\\Week 13, 14, 15, 16 - project month\\code\\frontend - falcon5ive\\src\\Components\\userImage\\userImage.js",["357","358"],"//styling\r\n// import style from \"./userImage.module.css\";\r\nimport React, { useState, useEffect } from \"react\";\r\nimport \"./userImage.css\";\r\n\r\n//Link\r\nimport { Link } from \"react-router-dom\";\r\n\r\n// Styling\r\nimport style from \"./userImage.module.css\";\r\nimport cn from \"classnames\";\r\n\r\nexport default function UserImage({ user, width = \"60px\", centre, alumni }) {\r\n  const { profileimage, profileImage, name, cohort, id, uid } = user;\r\n  const [styling, setStyling] = useState(null);\r\n  const [cohortNumber, setCohortNumber] = useState(null);\r\n\r\n  function styleCohort() {\r\n    if (cohort === 1) {\r\n      setStyling(\"cohort1\");\r\n      setCohortNumber(1);\r\n    } else if (cohort === 2) {\r\n      setStyling(\"cohort2\");\r\n      setCohortNumber(2);\r\n    } else if (cohort === 3) {\r\n      setStyling(\"cohort3\");\r\n      setCohortNumber(3);\r\n    } else if (cohort === 4) {\r\n      setStyling(\"cohort4\");\r\n      setCohortNumber(4);\r\n    } else if (cohort === 5) {\r\n      setStyling(\"cohort5\");\r\n      setCohortNumber(5);\r\n    }\r\n  }\r\n\r\n  useEffect(() => {\r\n    styleCohort();\r\n  }, []);\r\n\r\n  let smallWidth = parseInt(width) / 4.5;\r\n  // style={{\r\n  //   width: `${smallWidth}px`,\r\n  //   height: `${smallWidth}px`,\r\n  //   fontSize: `${smallWidth}px`,\r\n  // }}\r\n\r\n  return (\r\n    <div\r\n      style={{ width: `${width}` }}\r\n      className={cn(style.image, centre && style.imageMargin)}\r\n    >\r\n      <Link to={`/bootcamper/${id ? id : uid}`}>\r\n        <img\r\n          class={styling}\r\n          src={profileImage ? profileImage : profileimage}\r\n          alt={`${name} profile`}\r\n          width={width}\r\n          className={`profImg ${styling}`}\r\n        />\r\n      </Link>\r\n      {!alumni && (\r\n        <div className={cn(style.cohort, styling)}>{cohortNumber}</div>\r\n      )}\r\n    </div>\r\n  );\r\n}\r\n","C:\\Users\\herzallr\\Desktop\\Me\\School of code\\Notes and code\\Week 13, 14, 15, 16 - project month\\code\\frontend - falcon5ive\\src\\Components\\UserInfo\\userinfo.js",["359"],"import SocialMedia from \"../UserSocial/social\";\r\n\r\n//styling\r\nimport style from \"./userInfo.module.css\";\r\