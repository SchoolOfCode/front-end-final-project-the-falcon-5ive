en((data) => console.log(\"this is the user data: \", data))\n        .catch((error) => console.log(\"user creation error error: \", error));\n\n      setComplete(true);\n    }\n  };\n\n  function consoleLog() {\n    console.log(user);\n    console.log(marker);\n  }\n\n  if (!complete) {\n    return (\n      <div>\n        <button onClick={consoleLog}>Get User</button>\n        <form onSubmit={handleSubmit(createEvent)}>\n          {eventsEdit && (\n            <button\n              onClick={() =>\n                hide === \"show\" ? setHide(\"hide\") : setHide(\"show\")\n              }\n            >\n              x\n            </button>\n          )}\n          <span>\n            <p>Event Name:</p>\n            <input\n              name=\"eventName\"\n              ref={register}\n              required\n              placeholder={eventname}\n            />\n          </span>\n          <span>\n            <p>Event Type:</p>\n            <select id=\"eventTypes\" name=\"eventTypes\" ref={register}>\n              <option value=\"education\">Education</option>\n              <option value=\"social\">Social</option>\n              <option value=\"community\">Community</option>\n            </select>\n          </span>\n          <span>\n            <p>Date:</p>\n            <input name=\"date\" type=\"date\" ref={register} required />\n          </span>\n          <span>\n            <p>Time:</p>\n            <input name=\"time\" type=\"time\" ref={register} required />\n          </span>\n          <span>\n            <p>Description:</p>\n            <textarea\n              name=\"description\"\n              rows=\"10\"\n              cols=\"30\"\n              ref={register}\n              placeholder={description}\n            ></textarea>\n          </span>\n          <span>\n            <p>Image:</p>\n            <input name=\"image\" ref={register} required />\n          </span>\n          <span>\n            <p>Location:</p>\n            <div>\n      